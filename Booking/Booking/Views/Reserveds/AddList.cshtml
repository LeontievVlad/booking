@model Booking.Models.ReservedViewModels.CreateReserveViewModel
@{
    ViewBag.Title = "AddList";
}


<br />
@using (Html.BeginForm())
{


    <p>
        @*@Model.ReservedId*@
        <br />
        @*@Model.RoomId*@
        <br />
        @*@Model.EventName*@
    </p>


    @Html.DropDownList("UsersEmails", new SelectList(Model.UsersEmails,null), new { style = "width: 300px;" })


    <input type="submit" value="submit" onclick="this.form.submit();" />
}
<br />
<br />

@foreach (var emails in Model.UsersEmails)
{
    <p>@emails</p>
}